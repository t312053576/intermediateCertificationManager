<generatorConfiguration>
  <classPathEntry location="C:\Users\Administrator\.m2\repository\com\oracle\ojdbc6\11.2.0.2.0\ojdbc6-11.2.0.2.0.jar" />
    <context id="DB2Tables" targetRuntime="MyBatis3" defaultModelType="flat">
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
		<!-- 生成的bean实现序列化 -->
    	<plugin type="org.mybatis.generator.plugins.SerializablePlugin" /> 
        <!--去除注释 （true好像不起作用） -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

        <jdbcConnection driverClass="oracle.jdbc.OracleDriver"
                        connectionURL="jdbc:oracle:thin:@10.10.4.18:1521:orcl"
                        userId="rsbase"
                        password="123456">
        </jdbcConnection>
		
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		
		<!--生成实体类 Model指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建 使用Maven生成在target目录下，会自动创建） -->
        <javaModelGenerator targetPackage="com.weiboinfo.rsjBaseService.entry" targetProject="src/main/java"/>

		<!--生成SQLMAP文件 -->
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources"/>
		
		<!--生成Dao文件 可以配置 type="XMLMAPPER"生成xml的dao实现 context id="DB2Tables" 修改targetRuntime="MyBatis3"  com.base.datecenter.talentDemand.dao -->
